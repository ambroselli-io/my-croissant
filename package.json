{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix watch\"",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node server/src/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/src/index.js"
  },
  "dependencies": {
    "@remix-run/express": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@sentry/node": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fintecture-client": "^2.0.1",
    "helmet": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "localized-address-format": "^1.1.0",
    "mapbox-gl": "^2.6.1",
    "md5": "^2.3.0",
    "mongoose": "^6.1.5",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "tailwindcss": "^3.0.12"
